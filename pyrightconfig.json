{
  "venvPath": ".",
  "venv": "venv",
  "pythonVersion": "3.10",
  "pythonPlatform": "Linux",
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    ".git",
    "venv"
  ],
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.10",
      "extraPaths": [
        "./apps",
        "./config"
      ]
    }
  ],
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "typeCheckingMode": "basic",
  "useLibraryCodeForTypes": true,
  "autoSearchPaths": true,
  "stubPath": "./typings"
}
